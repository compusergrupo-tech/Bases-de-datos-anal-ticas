);

CREATE TABLE tienda (
  ID_Tienda INT PRIMARY KEY,
  Nombre VARCHAR(100),
  Ciudad VARCHAR(50),
  Región VARCHAR(50)
);

CREATE TABLE fecha (
  Fecha DATE PRIMARY KEY,
  Año INT,
  Mes INT,
  Dia INT,
  Nombre_mes VARCHAR(20)
);

CREATE TABLE venta (
  ID_Venta INT PRIMARY KEY,
  Fecha DATE,
  ID_Producto INT,
  ID_Tienda INT,
  Cantidad INT,
  PrecioUnitario DECIMAL(10,2)
);

