    <connection>bds_ventas</connection>
    <sql>SELECT t.ciudad, SUM(v.cantidad * v.preciounitario) AS ingresos
FROM ventas v
JOIN tiendas t ON v.id_tienda = t.id_tienda
GROUP BY t.ciudad
ORDER BY ingresos DESC;
