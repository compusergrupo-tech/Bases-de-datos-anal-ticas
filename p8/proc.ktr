    <connection>bds_ventas</connection>
    <sql>SELECT p.nombre,f.Nombre_mes, SUM(v.cantidad) AS total_vendidos
FROM ventas v
JOIN productos p ON v.ID_Producto = p.ID_Producto
JOIN fechas f ON v.fecha = f.fecha
WHERE f.Nombre_mes IN ('Octubre', 'Noviembre', 'Diciembre')
GROUP BY p.nombre, f.Nombre_mes
ORDER BY total_vendidos DESC;
</sql>
